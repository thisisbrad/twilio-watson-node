{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "arrowFunctions": true
    }
  },
  "rules": {
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "react/prefer-stateless-function": 1,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
